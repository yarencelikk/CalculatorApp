<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Calculator2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Calculator2"
    Shell.FlyoutBehavior="Flyout"
    Title="Calculator2">

    <ShellContent
        Title="Standard"
        ContentTemplate="{DataTemplate local:StandardPage}"
        Route="StandardPage" />

    <ShellContent
        Title="Scientific"
        ContentTemplate="{DataTemplate local:ScientificPage}"
        Route="ScientificPage" />

</Shell>
